{
    "standard.enable": false,
    "standard.autoFixOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "stylelint.enable": false,
    "stylelint.validate": [
        "css"
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
}